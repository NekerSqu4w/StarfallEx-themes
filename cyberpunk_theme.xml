<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">

<!-- this theme is based on this visual studio theme: https://marketplace.visualstudio.com/items?itemName=T0uchM3.CTVS19&ssr=false#overview -->
<!-- this may be not accurate due to StarfallEx editor limitation -->

<plist version="1.0">
<dict>
	<key>name</key>
	<string>Cyberpunk Theme</string>
	<key>gutterSettings</key>
	<dict>
		<key>background</key>
		<string>#000b1e</string>
		<key>foreground</key>
		<string>#ff007f</string>
		<string>divider</string>
		<string>#007379</string>
	</dict>
	<key>settings</key>
	<array>
		<dict>
			<key>settings</key>
			<dict>
				<key>background</key>
				<string>#000b1e</string>
				<key>caret</key>
				<string>#ffffff</string>
				<key>foreground</key>
				<string>#ffffff</string>
				<key>invisibles</key>
				<string>#a0e6ff</string>
				<key>lineHighlight</key>
				<string>#1a1c40</string>
				<key>selection</key>
				<string>#333366</string>
			</dict>
		</dict>
		<dict>
			<!-- single / multi line comment -->
			<key>name</key>
			<string>Comment</string>
			<key>scope</key>
			<string>comment</string>
			<key>settings</key>
			<dict>
				<key>foreground</key>
				<string>#0091ff</string>
			</dict>
		</dict>
		<dict>
			<!-- string "your text" -->
			<key>name</key>
			<string>String</string>
			<key>scope</key>
			<string>string</string>
			<key>settings</key>
			<dict>
				<key>foreground</key>
				<string>#00f7ff</string>
			</dict>
		</dict>
		<dict>
			<!-- function like print() -->
			<key>name</key>
			<string>Function</string>
			<key>scope</key>
			<string>function</string>
			<key>settings</key>
			<dict>
				<key>foreground</key>
				<string>#ffea50</string>
			</dict>
		</dict>
		<dict>
			<!-- library. like 'render.' -->
			<key>name</key>
			<string>Class name</string>
			<key>scope</key>
			<string>library</string>
			<key>settings</key>
			<dict>
				<key>foreground</key>
				<string>#ff66ff</string>
			</dict>
		</dict>
		<dict>
			<!-- constants -->
			<key>name</key>
			<string>Built-in constant</string>
			<key>scope</key>
			<string>constant</string>
			<key>settings</key>
			<dict>
				<key>foreground</key>
				<string>#c973ff</string>
			</dict>
		</dict>
		<dict>
			<!-- function, local -->
			<key>name</key>
			<string>Storage type</string>
			<key>scope</key>
			<string>storage</string>
			<key>settings</key>
			<dict>
				<key>foreground</key>
				<string>#a162ff</string>
			</dict>
		</dict>
		<dict>
			<!-- while, if, then, else-->
			<key>name</key>
			<string>Keyword</string>
			<key>scope</key>
			<string>keyword</string>
			<key>settings</key>
			<dict>
				<key>foreground</key>
				<string>#a162ff</string>
			</dict>
		</dict>
		<dict>
			<!-- number literal -->
			<key>name</key>
			<string>Number</string>
			<key>scope</key>
			<string>number</string>
			<key>settings</key>
			<dict>
				<key>foreground</key>
				<string>#ffdd33</string>
			</dict>
		</dict>
		<dict>
			<!-- a variable -->
			<key>name</key>
			<string>Variable</string>
			<key>scope</key>
			<string>variable</string>
			<key>settings</key>
			<dict>
				<key>foreground</key>
				<string>#ff007f</string>
			</dict>
		</dict>
		<dict>
			<!-- all operators like +-*=<> -->
			<key>name</key>
			<string>Operators</string>
			<key>scope</key>
			<string>operator</string>
			<key>settings</key>
			<dict>
				<key>foreground</key>
				<string>#ceeef0</string>
			</dict>
		</dict>
	</array>

	<key>uuid</key>
	<string>233F0694-0B9C-43E3-A44A-ECECF7DF6C73</string>
</dict>
</plist>